{
  "name": "api-doc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && touch out/.nojekyll && echo 'docs.diagrams-technologies.com' > out/CNAME",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "^18.15.12",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "front-matter": "^4.0.2",
    "glob": "^10.2.6",
    "inline-style-prefixer": "^7.0.0",
    "next": "^13.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "remark-parse": "^10.0.2",
    "swagger-ui-react": "^4.18.3",
    "unified": "^10.1.2",
    "yerror": "^6.2.1"
  },
  "homepage": "https://docs.diagrams-technologies.com",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/node": "^7.20.7",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "jest": "^29.5.0",
    "prettier": "2.8.7",
    "typescript": "^5.0.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "react/prop-types": "off",
      "curly": "error",
      "react/react-in-jsx-scope": "off",
      "react/no-unknown-property": [
        "error",
        {
          "ignore": [
            "jsx",
            "global"
          ]
        }
      ],
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "_",
          "varsIgnorePattern": "_"
        }
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "jest": true,
      "node": true
    }
  }
}
